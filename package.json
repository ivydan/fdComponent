{
  "name": "fdcomponent",
  "version": "0.1.9",
  "description": "webComponent",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivydan/fdComponent.git"
  },
  "author": "fdDan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivydan/fdComponent/issues"
  },
  "homepage": "https://github.com/ivydan/fdComponent#readme",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build",
    "prepublishC": "npm test && npm run clean && npm run build",
    "start": "webpack-dev-server --config ./webpack.config.dev.js --port 3000",
    "buildW": "webpack --config ./webpack.config.dev.js",
    "test": "./node_modules/.bin/karma start --hostname localhost",
    "test:local": "mocha --compilers js:babel-register --require test/dom.js --recursive",
    "test:cross-browser": "./node_modules/.bin/karma start --single-run --browsers F2E-Chrome,F2E-Edge,F2E-IE9,F2E-IE10,F2E-IE11",
    "ci-log:coverage": "./node_modules/.bin/ci-log -r coverage -p ./coverage/lcov.info",
    "ci-log:json": "./node_modules/.bin/ci-log -r json -p ./results.json",
    "storybook": "start-storybook -p 23232 -c .storybook"
  },
  "dependencies": {
    "exenv": "^1.2.1",
    "shallowequal": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.2.0",
    "enzyme": "^2.7.0",
    "isparta": "^4.0.0",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.4.3"
  },
  "files": [
    "README.md",
    "lib/"
  ]
}
